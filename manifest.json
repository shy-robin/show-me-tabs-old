{
  "manifest_version": 3,
  "name": "Show Me Tabs",
  "description": "Show me tabs",
  "version": "1.0",
  "action": {
    "default_popup": "./popup/popup.html",
    "default_icon": "./images/hi.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["https://developer.chrome.com/*"],
  "permissions": ["tabGroups", "notifications", "tabs"]
}
